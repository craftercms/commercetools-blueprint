                  </property>
                  <property>
                     <name>thumbnailHeight</name>
                     <value/>
                     <type>int</type>
                  </property>
                  <property>
                     <name>imageManager</name>
                     <value>upload,browse</value>
                     <type>datasource:image</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>input</type>
               <id>title_s</id>
               <iceId/>
               <title>Title</title>
               <description/>
               <defaultValue/>
               <help/>
