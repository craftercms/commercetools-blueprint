                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>tokenize</name>
                     <value>false</value>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value/>
                     <type>boolean</type>
                  </constraint>
                  <constraint>
                     <name>pattern</name>
                     <value/>
                     <type>string</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>rte</type>
               <id>content_html</id>
               <iceId/>
               <title>Content</title>
               <description/>
               <defaultValue/>
