               <help/>
               <properties/>
               <constraints/>
            </field>
            <field>
               <type>input</type>
               <id>internal-name</id>
               <iceId/>
               <title>Internal Name</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>image-picker</type>
               <id>image_s</id>
               <iceId/>
