                  </property>
                  <property>
                     <name>rteConfiguration</name>
                     <value>generic</value>
                     <type>string</type>
                  </property>
                  <property>
                     <name>imageManager</name>
                     <value/>
                     <type>datasource:image</type>
                  </property>
                  <property>
                     <name>videoManager</name>
                     <value/>
                     <type>datasource:video</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>dropdown</type>
               <id>contentAlignment_s</id>
               <iceId/>
               <title>Content Alignment</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>datasource</name>
                     <value>alignments</value>
                     <type>datasource:item</type>
                  </property>
                  <property>
                     <name>emptyvalue</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>image-picker</type>
               <id>section_background_image_s</id>
               <iceId/>
               <title>Section Background Image</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>width</name>
                     <value>{ "exact":"", "min":"", "max":"" }</value>
                     <type>range</type>
                  </property>
                  <property>
                     <name>height</name>
                     <value>{ "exact":"", "min":"", "max":"" }</value>
                     <type>range</type>
                  </property>
                  <property>
                     <name>thumbnailWidth</name>
                     <value/>
                     <type>int</type>
                  </property>
                  <property>
                     <name>thumbnailHeight</name>
                     <value/>
                     <type>int</type>
                  </property>
                  <property>
                     <name>imageManager</name>
                     <value>upload,browse</value>
                     <type>datasource:image</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value/>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
         </fields>
      </section>
   </sections>
   <datasources>
      <datasource>
         <type>img-desktop-upload</type>
         <id>upload</id>
         <title>Upload</title>
         <interface>image</interface>
         <properties>
            <property>
               <name>repoPath</name>
               <value>/static-assets/images/page/{objectId}</value>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
      <datasource>
         <type>img-repository-upload</type>
         <id>browse</id>
         <title>Browse</title>
         <interface>image</interface>
         <properties>
            <property>
               <name>repoPath</name>
               <value>/static-assets/images</value>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
      <datasource>
         <type>key-value-list</type>
         <id>alignments</id>
         <title>Alignments</title>
         <interface>item</interface>
         <properties>
            <property>
               <name>dataType</name>
               <value>[{"value":"value","label":"","selected":true},{"value":"value_s","label":"String","selected":false},{"value":"value_i","label":"Integer","selected":false},{"value":"value_f","label":"Float","selected":false},{"value":"value_dt","label":"Date","selected":false},{"value":"value_html","label":"HTML","selected":false}]</value>
               <type>undefined</type>
            </property>
            <property>
               <name>options</name>
               <value>[{"key":"left","value":"Left"},{"key":"right","value":"Right"}]</value>
               <type>undefined</type>
            </property>
            <property>
               <name>showkeys</name>
               <value/>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
   </datasources>
</form>
