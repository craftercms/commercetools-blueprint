                  <property>
                     <name>width</name>
                     <value>{ "exact":"", "min":"", "max":"" }</value>
                     <type>range</type>
                  </property>
                  <property>
                     <name>height</name>
                     <value>{ "exact":"", "min":"", "max":"" }</value>
                     <type>range</type>
                  </property>
                  <property>
                     <name>thumbnailWidth</name>
                     <value/>
