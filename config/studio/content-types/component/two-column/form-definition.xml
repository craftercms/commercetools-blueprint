<form>
   <title>Component - Two Column</title>
   <description/>
   <objectType>component</objectType>
   <content-type>/component/two-column</content-type>
   <imageThumbnail>undefined</imageThumbnail>
   <quickCreate>false</quickCreate>
   <quickCreatePath/>
   <properties>
      <property>
         <name>display-template</name>
         <label>Display Template</label>
         <value/>
         <type>template</type>
      </property>
      <property>
         <name>merge-strategy</name>
         <label>Merge Strategy</label>
         <value>inherit-levels</value>
         <type>string</type>
      </property>
      <property>
         <name>no-template-required</name>
         <label>No Template Required</label>
         <type>boolean</type>
         <value/>
      </property>
   </properties>
   <sections>
      <section>
         <title>Two Column Properties</title>
         <description/>
         <defaultOpen>true</defaultOpen>
         <fields>
            <field>
               <type>auto-filename</type>
               <id>file-name</id>
               <iceId/>
